c\",\n          \"#fb9a99\",\n        ]);\n\n        var pie = d3\n          .pie()\n          .value((d) => d.rate)\n          .sort(null)\n          .sortValues(d3.descending);\n\n        var path = d3.arc().outerRadius(radius).innerRadius(0);\n\n        var label = d3\n          .arc()\n          .outerRadius(radius - 40)\n          .innerRadius(radius - 40);\n\n        var arc = g\n          .selectAll(\".arc\")\n          .data(pie(data))\n          .enter()\n          .append(\"g\")\n          .attr(\"class\", \"arc\")\n          .on(\"mouseover\", function (d) {\n            d3.select(this)\n              .select(\"path\")\n              .attr(\"stroke\", \"black\")\n              .attr(\"stroke-width\", 5);\n            var x = d3.select(this).select(\"g.arc > text\").text();\n\n            //.select('#tooltip').transition().duration(1).style('opacity', 1).text(d.country)\n            d3.select(\"#tooltip\")\n              .style(\"left\", d.pageX + \"px\")\n              .style(\"top\", d.pageY + \"px\")\n              .style(\"opacity\", 1)\n              .select(\"#value\")\n              .text(x);\n          })\n          .on(\"mouseout\", function () {\n            d3.select(this).select(\"path\").attr(\"stroke-width\", 0);\n            d3.select(\"#tooltip\").style(\"opacity\", 0);\n          });\n        arc\n          .append(\"path\")\n          .attr(\"d\", path)\n          .attr(\"fill\", (d) => color(d.data.rate));\n\n        arc\n          .append(\"text\")\n          .attr(\"transform\", (d) => \"translate(\" + label.centroid(d) + \")\")\n          .attr(\"class\", \"pies\")\n          .text((d) => d.data.certificate + \":\" + d.data.rate);\n      });\n    },\n    myPieCo() {\n      var slice = {\n        //a slice of pie\n        innerRadius: 0,\n        outerRadius: 100,\n        startAngle: 0,\n        endAngle: Math.PI / 2,\n      };\n\n      var arc = d3.arc();\n      console.log(arc(slice));\n\n      var svg = d3.select(\"#pies\"),\n     